#Problem - 1 solution

class Solution(object):
    def rotate(self, nums, k):
         n = len(nums)
         k %= n 
         def reverse(st, end):
            while st < end:
                nums[st], nums[end] = nums[end], nums[st]
                st += 1
                end -= 1
    
         reverse(0, n - 1)
    
         reverse(0, k - 1)
    
         reverse(k, n - 1)
        
