#Problem 3 solution

class Solution(object):
    def singleNumber(self, nums):
        res = 0
        for i in range(32):  
            bit_count = 0
            for num in nums:
                if (num >> i) & 1:
                    bit_count += 1
            if bit_count % 3:
                #negative numbers
                if i == 31: 
                    res -= (1 << i)
                else:
                    res |= (1 << i)
        return res
        
